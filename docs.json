{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Spring AI Community",
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "favicon": "/favicon.svg",
  "colors": {
    "primary": "#6DB33F",
    "light": "#77BC3F",
    "dark": "#5FA134",
    "background": {
      "light": "#FFFFFF",
      "dark": "#0D1117"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Community",
        "groups": [
          {
            "group": "About",
            "pages": [
              "community/index",
              "community/governance",
              "community/project-guide",
              "community/releases",
              "community/integration"
            ]
          }
        ]
      },
      {
        "tab": "Projects",
        "groups": [
          {
            "group": "Overview",
            "pages": [
              "projects/index"
            ]
          },
          {
            "group": "Production Projects",
            "pages": [
              "projects/production/moonshot",
              "projects/production/qianfan"
            ]
          },
          {
            "group": "Incubating Projects",
            "pages": [
              "projects/incubating/spring-ai-agents",
              "projects/incubating/spring-ai-bench",
              "projects/incubating/mcp-annotations",
              "projects/incubating/mcp-security"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Benchmarking Initiative",
          "href": "/benchmarking",
          "icon": "chart-line"
        },
        {
          "anchor": "Get Involved",
          "href": "/get-involved",
          "icon": "users"
        },
        {
          "anchor": "GitHub",
          "href": "https://github.com/spring-ai-community",
          "icon": "github"
        }
      ]
    }
  },
  "navbar": {
    "links": [
      {
        "label": "Spring AI",
        "href": "https://spring.io/projects/spring-ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Propose Project",
      "href": "https://github.com/spring-ai-community/community/issues/new?template=application.yml"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/spring-ai-community",
      "x": "https://x.com/springcentral"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "cursor"
    ]
  }
}
