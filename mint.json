                    "claude-agent-sdk/tutorial/06-cli-options",
                    "claude-agent-sdk/tutorial/07-tool-permissions",
                    "claude-agent-sdk/tutorial/08-permission-modes",
                    "claude-agent-sdk/tutorial/09-structured-outputs"
