{
  "$schema": "https://mintlify.com/schema.json",
  "name": "Spring AI Community",
  "ignore": ["plans/**", "README.md", "PROJECT_METADATA_TEMPLATE.md", "CLAUDE.md"],
  "logo": "/logo/universal.svg",
  "favicon": "/logo/favicon.svg",
  "colors": {
    "primary": "#4A90E2",
    "light": "#5BA3F5",
    "dark": "#3A7BC8"
